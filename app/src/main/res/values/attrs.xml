<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BadgeImageView">
        <attr name="android:text"/>
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
        <attr name="circleColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="DataWrapper">
        <attr name="minGridWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="DualHintEdit">
        <attr name="dualHint" format="string"/>
        <attr name="dualSize" format="dimension"/>
        <attr name="dualGap" format="dimension"/>
        <attr name="dualColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="IndicatorBlock">
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
        <attr name="android:textStyle"/>
        <attr name="cornerRadius" format="dimension"/>
        <attr name="spacingGap" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="PagerStripe">
        <attr name="stripeThickness" format="dimension"/>
        <attr name="android:textSize"/>
    </declare-styleable>
    <declare-styleable name="PlacesFieldView">
        <attr name="searchTypes" format="string"/>
        <attr name="manualPrompt" format="string"/>
        <attr name="android:dropDownWidth"/>
        <attr name="android:dropDownHorizontalOffset"/>
    </declare-styleable>
    <declare-styleable name="PriceSticker">
        <attr name="majorTextSize" format="dimension"/>
        <attr name="minorTextSize" format="dimension"/>
        <attr name="majorTextColor" format="color"/>
        <attr name="minorTextColor" format="color"/>
        <attr name="android:gravity"/>
    </declare-styleable>
    <declare-styleable name="QuantityEditor">
        <attr name="minQuantity" format="integer"/>
        <attr name="maxQuantity" format="integer"/>
        <attr name="popupWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="RatingStars">
        <attr name="android:textSize"/>
        <attr name="android:gravity"/>
        <attr name="noReviews" format="string"/>
    </declare-styleable>
    <declare-styleable name="TileLayout">
        <attr name="minHUnits" format="integer"/>
        <attr name="maxHUnits" format="integer"/>
        <attr name="hModulo" format="integer"/>
        <attr name="minHUnitSize" format="dimension"/>
    </declare-styleable>
</resources>
